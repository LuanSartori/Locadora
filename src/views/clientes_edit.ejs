<%- include('paginas/_header'); -%>

<div class="container mt-5">
    <div class="row">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/update/<%= data.clienteID %>" method="post">
                        <div class="form-group">
                            <input type="text" name="clienteNome" value="<%= data.clienteNome %>" class="form-control" >
                        </div>
                        <div class="form-group">
                            <input type="text" name="clienteEnde" value="<%= data.clienteEnde %>" class="form-control" >
                        </div>
                        <div class="form-group">
                            <input type="text" name="clienteTel" value="<%= data.clienteTel %>" class="form-control" >
                        </div>
                        <button type="submit" class="btn btn-primary">
                            Atualizar
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include('paginas/_footer'); -%>
